{
  "name": "digital-atlas",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:push": "dotenv -e .env.local -- prisma db push",
    "prisma:seed": "dotenv -e .env.local -- prisma db seed",
    "prisma:studio": "dotenv -e .env.local -- prisma studio",
    "setup:db": "npm run prisma:generate && npm run prisma:push && npm run prisma:seed"
  },
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@googlemaps/js-api-loader": "^1.16.7",
    "@googlemaps/markerclusterer": "2.6.1",
    "@mui/icons-material": "5.16.7",
    "@mui/material": "5.16.7",
    "@prisma/client": "5.20.0",
    "@react-google-maps/api": "2.19.3",
    "bcrypt": "5.1.1",
    "exceljs": "4.4.0",
    "next": "^14.2.32",
    "next-auth": "^4.24.11",
    "prisma": "5.20.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-google-recaptcha": "^3.1.0",
    "typescript": "5.6.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.2.15",
    "tsx": "^4.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=20.18.0"
  }
}
